<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

  <dubbo:application name="dubbo-provider"/>
  <dubbo:registry protocol="zookeeper" address="192.168.42.134:2181"/>
  <dubbo:protocol name="dubbo" port="20880"/>
  <dubbo:service interface="dubbo.common.service.HelloService" ref="helloService"/>
  <dubbo:service interface="dubbo.common.service.UserService" ref="userService"/>

  <bean id="helloService" class="dubbo.provider.service.impl.HelloServiceImpl"/>
  <bean id="userService" class="dubbo.provider.service.impl.UserServiceImpl"/>

</beans>
